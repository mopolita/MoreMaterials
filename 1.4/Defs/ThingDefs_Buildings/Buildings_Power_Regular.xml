  <!-- Ancient Battery-->
    <ThingDef ParentName="BuildingBase">
    <defName>BE_Ancient_Battery</defName>
    <label>ancient battery</label>
    <description>A battery developed by archotechs. It is sometimes dug up from ruins by travelling merchants.\nThis battery won't short-circuit in rain and not break down.\n\nIt is also useful to not that this battery actually store more energy than the amount you provide, thanks to it's ancient vanometric technology</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <thingClass>Building_Battery</thingClass>
    <graphicData>
      <texPath>Things/Building/Power/Regular_Batteries/BE_Ancient_Battery</texPath>
