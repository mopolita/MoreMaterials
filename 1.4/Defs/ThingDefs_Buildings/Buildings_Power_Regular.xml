<Defs>

  <!-- Regular Lead Battery -->
  <ThingDef ParentName="BE_BatteryBase">
    <defName>BE_Lead_Battery</defName>
    <label>lead battery</label>
    <description>Stores electricity for later use. Can store only a small amount of power and is not very efficient, but is very sturdy.</description>
    <graphicData>
      <texPath>Things/Building/Power/Regular_Batteries/BE_Lead_Battery</texPath>
    </graphicData>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>1000</WorkToBuild>
      <Mass>60</Mass>
      <Flammability>0.25</Flammability>
      <Beauty>-15</Beauty>
    </statBases>
    <descriptionHyperlinks>
      <ThingDef>BE_Copper_Battery</ThingDef>
      <ThingDef>Battery</ThingDef>
      <ThingDef>BE_Waterproof_Battery</ThingDef>
      <ThingDef>BE_Silverplated_Battery</ThingDef>
      <ThingDef>BE_Jadeplated_Battery</ThingDef>
      <ThingDef>BE_Goldplated_Battery</ThingDef>
      <ThingDef>BE_Nickel_Battery</ThingDef>
      <ThingDef>BE_Uranium_Battery</ThingDef>
      <ThingDef>BE_Lithium_Battery</ThingDef>
      <ThingDef>BE_Plasteel_Battery</ThingDef>
      <ThingDef>BE_Ancient_Battery</ThingDef>
    </descriptionHyperlinks>
    <costList>
      <ME_Lead>65</ME_Lead>
      <Steel>20</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <comps>
      <li Class="CompProperties_Battery">
        <storedEnergyMax>500</storedEnergyMax>
        <efficiency>0.5</efficiency>
        <shortCircuitInRain>true</shortCircuitInRain>
        <transmitsPower>true</transmitsPower>
      </li>  
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <researchPrerequisites>
      <li>BE_Lead_Batteries</li>
    </researchPrerequisites>
  </ThingDef>

